{
    "name": "url-shortener",
    "version": "0.1.5",
    "description": "URL shortener",
    "main": "dist/worker.js",
    "scripts": {
        "build": "webpack",
        "dev": "cross-env NODE_ENV=development npm run build",
        "format": "prettier-eslint --write \"%INIT_CWD%/**/*.{ts,js,css,json,md}\" && eslint --fix src",
