        "@types/chai": "^4.2.14",
        "@types/expect": "^24.3.0",
        "@types/mocha": "^8.0.4",
        "@types/node": "^14.14.9",
        "@types/service-worker-mock": "^2.0.1",
        "@typescript-eslint/eslint-plugin": "^4.8.1",
        "@typescript-eslint/parser": "^4.8.1",
        "chai": "^4.2.0",
        "cross-env": "^7.0.2",
        "eslint": "^7.14.0",
        "eslint-config-prettier": "^6.15.0",
        "mocha": "^8.2.1",
        "prettier": "^2.2.0",
        "prettier-eslint": "^12.0.0",
        "prettier-eslint-cli": "^5.0.0",
        "raw-loader": "^4.0.2",
        "rimraf": "^3.0.2",
        "service-worker-mock": "^2.0.5",
        "ts-loader": "^8.0.11",
        "ts-mocha": "^8.0.0",
        "tsconfig-paths": "^3.9.0",
        "typescript": "^4.1.2",
        "webpack": "^5.6.0",
        "webpack-cli": "^4.2.0"
